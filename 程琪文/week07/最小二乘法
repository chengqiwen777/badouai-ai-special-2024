import pandas as pd

table = pd.read_csv('train_data.csv')
X = table['X'].values
Y = table['Y'].values

s1 = 0
s2 = 0
s3 = 0
s4 = 0
n = X.shape[0]
# print(n)

for i in range(n):
    s1 += (X[i] * Y[i])
    s2 += (X[i] * X[i])
    s3 += X[i]
    s4 += Y[i]

k = (n * s1 - s3 * s4) / (n * s2 - s3 * s3)
b = (s4 - k * s3) / n

print("Coeff: {} Intercept: {}".format(k, b))



